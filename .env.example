# Environment variables for Colmado Backend

# Database Connection
# PostgreSQL example:
DATABASE_URL="postgresql://user:password@localhost:5432/colmado_db"
# MySQL example:
# DATABASE_URL="mysql://user:password@localhost:3306/colmado_db"

# JWT Secret Key (change this to a random secure string)
JWT_SECRET="change-this-to-a-secure-random-string"

# Application Port
PORT=3000

# Node Environment
NODE_ENV=development
